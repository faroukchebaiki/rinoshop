{
  "name": "rinoshop",
  "version": "0.1.0",
  "private": true,
  "description": "Modern marketplace for digital products.",
  "type": "module",
  "packageManager": "pnpm@10.18.2",
  "engines": {
    "node": ">=18.17.0"
  },
  "main": "dist/server.cjs",
  "scripts": {
    "dev": "nodemon",
    "build:server": "esbuild src/server.ts --bundle --platform=node --format=cjs --target=node20 --external:next --external:express --external:stripe --external:resend --external:dotenv --outfile=dist/server.cjs",
    "build:next": "cross-env NEXT_BUILD=true node dist/server.cjs",
    "build": "cross-env NODE_ENV=production pnpm build:server && pnpm copyfiles && pnpm build:next",
    "start": "cross-env NODE_ENV=production node dist/server.cjs",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,js,d.ts,json}\" dist/",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "pnpm": {
    "overrides": {
      "cliui": "7.0.2"
    },
    "onlyBuiltDependencies": [
      "@prisma/engines",
      "prisma"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@neondatabase/auth": "0.1.0-beta.18",
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/adapter-neon": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@prisma/client-runtime-utils": "^7.2.0",
    "@prisma/config": "^7.2.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-email/components": "^1.0.2",
    "@tanstack/react-query": "5.90.12",
    "@trpc/client": "^11.8.1",
    "@trpc/next": "^11.8.1",
    "@trpc/react-query": "^11.8.1",
    "@trpc/server": "^11.8.1",
    "@vercel/blob": "^2.0.0",
    "body-parser": "^2.2.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^10.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.0",
    "nodemailer": "^7.0.12",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.69.0",
    "resend": "^6.6.0",
    "sonner": "^2.0.7",
    "stripe": "^20.1.0",
    "swiper": "^12.0.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tsconfig-paths": "^4.2.0",
    "ws": "^8.18.3",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.23",
    "copyfiles": "^2.4.1",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.0",
    "nodemon": "^3.1.11",
    "postcss": "^8.5.6",
    "prisma": "^7.2.0",
    "tailwindcss": "^4.1.18",
    "tsc-alias": "^1.8.16",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}